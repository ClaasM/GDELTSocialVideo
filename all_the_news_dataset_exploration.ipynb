    "\n",
    "from zipfile import ZipFile\n",
    "import pandas as pd\n",
    "\n",
    "with ZipFile('data/all_the_news/all-the-news.zip') as zipfile:\n",
    "    files = zipfile.namelist()\n",
    "    for filename in files:\n",
    "        with zipfile.open(filename) as file:\n",
    "            pd.read_csv(file).head()"
